function logout()
{
  $.ajax({
    url: '<%= Rails.application.routes.url_helpers.ygg_session_logout_path %>',
    dataType: 'json',
    type: 'POST',
    contentType: 'application/json; charset=utf-8',
    data: JSON.stringify({}),
    success: function(data) {
      window.location = '/';
    },
  });
}
