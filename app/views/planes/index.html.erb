<div class="body_contents">
<% if !@targets.empty? then %>
  <table class="standard planes">
    <caption>Alianti</caption>
    <tr>
      <th>Marche</th>
      <th>Tipo</th>
    </tr>
    <% for x in @targets do %>
    <tr>
      <td class="registration"><%= link_to(x.registration, x) %></td>
      <td class="type"><%= link_to(x.plane_type.name, x.plane_type) %></td>
    </tr>
    <% end %>
  </table>
<% end %>
</div>
